pipeline {
    agent any
    stages {
        stage('Build Stage') {
            steps {
                echo "Calling rake task"
                bat 'rake --rakefile Manikanta/rakefile.rb cpp[%version_num%]'
                echo "Finished with rake"
            }
        }
        stage('success') {
            steps {
                echo "Successfully finished!!"
            }
        } 
    }
}
